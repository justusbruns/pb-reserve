{
  "rewrites": [
    { "source": "/en", "destination": "/" },
    { "source": "/en/(.*)", "destination": "/$1" },
    { "source": "/chauffeur", "destination": "/" },
    { "source": "/en/chauffeur", "destination": "/" }
  ],
  "cleanUrls": true,
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    { "src": "/api/calendar", "dest": "/api/calendar.ts" }
  ]
}
